<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSPY v2004 rel. 3 U (http://www.xmlspy.com) by Jan-Anders Jönsson (ÅKEJ AB) --><xs:schema targetNamespace="http://www.fi2.se/schemas/1.3" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.fi2.se/schemas/1.3" elementFormDefault="qualified" attributeFormDefault="unqualified" version="17.3">	<xs:include schemaLocation="./fi2base.xsd"/>	<xs:annotation>		<xs:documentation>FI2xml schema Building Energy Statement. Store and transport statements</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation>Based on fi2xml version 1.3, Boverket version 21</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation>Schema internal version 21</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation>Amendments:</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation>21 2013-04-30 FG@IB renamed to 21 to match internal version from Boverket</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 6.3 2013-04-17 FG@IB Removed fi2estatement_solarinstallation from fi2estatement_otherimprovements. refvalue2min and max now added again (were removed in Boverket version 1.8)</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 6.2 2012-12-19 FG@IB Removed everything under fi2Statment_aircond except fi2estatement_aircondexists</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 6.1 2012-12-05 FG@IB refvalue2min and refvalue2max removed. historicbuilding and valuablebuilding added. solarcollectorenergyproduction and solarcellelectricityproduction added. complex type fi2estatement_solarinstallation added to improvementtype- </xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 6.0 2012-12-04 FG@IB fi2xml 1.3 implemented</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 5.4 2012-06-26 DE@IB Removed fi2estatement_improveemission from fi2estatement_improvementtype</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 5.3 2009-04-15 Restrictions changed</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 5.2 2009-04-14 Test version released.</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 5.1 2009-04-07 Element names changed</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 5.0 2009-04-01 Element names changed</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 4.9 2009-03-30 Preliminary changes for 1.5 structure</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 4.8 2008-11-19 Tenure form removed. fi2estatement_owneroccupied added</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 4.7 2008-11-12 Rev 1.4 mods</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 4.6 2008-11-03 Rev 1.4 mods</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 4.5 2008/-03-20 Rev 1.3 implemented</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 4.4 2008-02-06 Minor mods</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 4,3 2008-02-03 Annotations adjusted. New sttributes for fi2estatement_detailinfo</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 4.2 2008-01-28 fi2estatement_compexity changed to fi2estatement_complexstructure</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 4,1 2008-01-25 New elements: fi2estatement_basementcount, fi2estatement_complexity</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 4.0 2007-11-06 All fi2estatemet tags set to optional</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 3.9 2007-10-18 fi2estatement_structcat changed to optional</xs:documentation>	</xs:annotation>	<xs:annotation>		<xs:documentation> 3.8 2007-10-05 fi2structure replaced with